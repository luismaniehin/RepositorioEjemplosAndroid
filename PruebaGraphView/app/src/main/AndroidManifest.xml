<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ricopollo.lnieto.pruebagraphview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OptionActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" /> <!-- gráfico dispersión -->
        <activity android:name=".Main2Activity" /> <!-- gráfico barras scrollview -->
        <activity android:name=".Main3Activity" /> <!-- gráfico barras secciones -->
        <activity android:name=".Main4Activity" /> <!-- gráfico barras dividido -->
        <activity android:name=".Main5Activity" /> <!-- Lista gráficos de barra -->
        <!-- <activity android:name=".Main6Activity" />&lt;!&ndash;sensor tiempo real&ndash;&gt; -->
        <activity android:name=".Main6Activity"></activity>
    </application>

</manifest>